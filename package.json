{
  "name": "@dattn/dnd-grid",
  "version": "0.0.10",
  "description": "A vuejs grid with draggable and resizable boxes",
  "author": "Daniel Duton <daniel@duton.lu>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dattn/dnd-grid.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "./node_modules/poi/bin/cli.js build ./src/index.js --dist ./dist/ --prod --library",
    "build:demo": "./node_modules/poi/bin/cli.js build ./src/demo/index.js --dist ./demo/ --prod",
    "start": "npm run dev",
    "dev": "./node_modules/poi/bin/cli.js ./src/demo/index.js",
    "lint": "./node_modules/eslint/bin/eslint.js --ext .js,.vue src"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "vue",
    "component"
  ],
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.2.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "poi": "^9.6.13"
  }
}
