{
  "name": "@dattn/dnd-grid",
  "version": "0.0.15",
  "description": "A vuejs grid with draggable and resizable boxes",
  "author": "Daniel Duton <daniel@duton.lu>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dattn/dnd-grid.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "dist": "poi ./src/index.js --out-dir ./dist/ --prod --clean --format umd --module-name dnd-grid",
    "dist:demo": "poi ./src/demo/index.js --out-dir ./demo/ --clean --prod",
    "build": "npm run dist -- --no-minimize",
    "build:demo": "npm run dist:demo -- --no-minimize",
    "start": "npm run dev",
    "dev": "poi --serve ./src/demo/index.js",
    "lint": "eslint --ext .js,.vue src"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "vue",
    "component"
  ],
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.5.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "poi": "^12.7.2"
  },
  "dependencies": {
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  }
}
