{
    "name": "@dattn/dnd-grid",
    "version": "0.0.4",
    "description": "A vuejs grid with draggable and resizable boxes",
    "author": "Daniel Duton <daniel@duton.lu>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dattn/dnd-grid.git"
    },
    "main": "dist/dnd-grid.js",
    "scripts": {
        "dist": "vue build ./src/index.js --config ./build.config.js --dist ./dist/ --prod --lib",
        "dist:demo": "vue build ./src/demo.vue --config ./build.config.js --dist ./demo/ --prod",
        "build": "npm run dist -- --disable-compress",
        "start": "npm run dev",
        "dev": "vue build ./src/demo.vue --config ./build.config.js",
        "demo:open": "open 'http://localhost:3000/'; serve ./demo/"
    },
    "files": [
        "dist/"
    ],
    "keywords": [
        "vue",
        "component"
    ]
}
